  "server_module_config": [
    // 有的模块只有名称，没有需要运行的process，只是作为一个模块标记，没有被单独起服务的必要，进作为流程标记，便于用户区分流程
    // function_name 以 功能+运行平台类型 命名，如 python_linux_server
    {
      "process_name": "python_api_server", // 进程名称
      "function_name": "python_linux_server", // 进程功能名称(同一个功能可能有多个实例，如API服务进程，共用一个function_name，但有多个process_name，区分不同实例资源limit)
      "max_instance_count": 5, // 最大实例数量
      "module_id": 1, // 所属模块ID
      "module_name": "PYTHON_API服务模块", // 所属模块名称
      "limit_cpu_cores": 64, // CPU核心数限制
      "limit_ram_size_gb": 128, // 内存大小限制，单位GB
      "limit_disk_size_gb": 200, // 磁盘大小限制，单位GB
      "desc": "API服务进程", // 进程描述
      "cmdline": "/usr/local/bin/request_api_mq_server",
      "run_server_type": "linux", // 进程运行服务器类型，linux/windows
      "isroot": 1, // 是否需要root权限运行，1: 是，0：否
      "platform_type": 1 // 1：智能结构仿真平台，2：实验室测试平台， 3：资料平台
    },
    {
      "process_name": "solver_worker", // 进程名称
      "function_name": "solver_linux_server", // 进程功能名称(同一个功能可能有多个实例，如API服务进程，共用一个function_name，但有多个process_name，区分不同实例资源limit)
      "max_instance_count": 20, // 最大实例数量
